{
	"name": "dxdevcheck",
	"displayName": "dxdevcheck",
	"description": "checks dev things",
	"version": "1.0.0",
	"publisher": "trebleCode",
	"engines": {
		"vscode": "^1.28.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.checkDirectories"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.checkDirectories",
				"title": "DX DevCheck: Check Directories"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"appdata-path": "^1.0.0"
	}
}
