{
	"name": "dxdevcheck",
	"displayName": "dxdevcheck",
	"description": "checks dev things",
	"version": "0.0.1",
	"publisher": "scottsweeney",
	"engines": {
		"vscode": "^1.28.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.runDevChecker"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.runDevChecker",
				"title": "DX DevCheck: Run Config Checker"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.32",
		"@types/node": "^6.0.40",
		"config": "^3.0.1",
		"configparser": "^0.2.6",
		"tslint": "^5.8.0",
		"typescript": "^2.0.3",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"ini": "^1.3.5",
		"jfile": "^1.1.12"
	}
}
